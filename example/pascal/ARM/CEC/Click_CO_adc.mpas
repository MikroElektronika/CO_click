unit Click_CO_adc;

uses Click_CO_types;

procedure co_adcInit(); 
function  co_readADC() :  uint32_t; 

implementation

procedure co_adcInit(); 
begin

    ADC1_Init(); 
    ADC_Set_Input_Channel(0x01); 

end;

function  co_readADC() :  uint32_t; 
var
    readADC : uint32_t;
begin

    readADC := ADC1_Read(1); 
    result := readADC; 

end;

end.