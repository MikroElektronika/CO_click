module Click_CO_adc

include Click_CO_types

sub procedure co_adcInit() 
sub function  co_readADC() as uint32_t 

implements

sub procedure co_adcInit() 

    ADC_Init() 
    ADC_Set_Input_Channel(_ADC_CHANNEL_4) 

end sub

sub function  co_readADC() as uint32_t 
dim
    readADC as uint32_t

    readADC = ADC_Read(4) 
    result = readADC 

end sub

end.